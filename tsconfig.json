{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "types": [ "vite/client" ],
    "paths": {
      "@/*": [ "./*" ]
    }
  },
  "include": [
    "**/*.d.ts",
    "**/*.ts",
    "**/*.vue"
  ],
  "exclude": [
    "node_modules",
    ".vitepress/dist",
    ".vitepress/cache"
  ],
  "extensions": [
    ".ts",
    ".vue"
  ],
  "allowSyntheticDefaultImports": true
}